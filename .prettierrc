{
    "trailingComma": "all",
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "jsxSingleQuite": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "overrides:": [
        {
            "files": "*.sol",
            "options": {
                "printWidth": 80,
                "useTabs": false,
                "tabWidth": 4,
                "singleQuote": false,
                "bracketSpacing": false,
                "explicitTypes": "always"
            }
        }
    ]
}